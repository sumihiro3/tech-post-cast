{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.8.0",
    "generators": {
      "lp": {
        "generatorName": "typescript-axios",
        "inputSpec": "apps/api-backend/api-spec/for-frontend-api.api-spec.json",
        "output": "apps/lp-frontend/src/api"
      },
      "liff": {
        "generatorName": "typescript-axios",
        "inputSpec": "apps/line-bot/api-spec/liff.api-spec.json",
        "output": "apps/liff-frontend/src/api"
      }
    }
  }
}
